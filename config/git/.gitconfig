[user]
  name = StefanoMagrassi
  email = 1291271+StefanoMagrassi@users.noreply.github.com
[core]
  editor = vim
[push]
  default = simple
[merge]
  tool = meld
[diff]
  tool = meld
[color]
  ui = auto
[alias]
  pr = pull --rebase
  co = checkout
  br = branch
  ci = commit
  st = status
  lg = log --oneline -n10
  unstage = reset HEAD --
  last = log -1 HEAD
  visual = !gitk
  short = log --abbrev-commit --pretty=oneline
  tree = log --graph --pretty=oneline --abbrev-commit
  rmremote = push origin --delete
  mt = mergetool
[mergetool]
  keepBackup = false
[init]
	defaultbranch = main
[pull]
	rebase = false
