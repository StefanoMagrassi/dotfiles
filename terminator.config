[global_config]
  always_split_with_profile = True
  title_hide_sizetext = True
[keybindings]
  edit_tab_title = <Primary><Shift>s
  edit_terminal_title = <Primary><Alt>x
  toggle_scrollbar = <Primary><Alt>a
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = default
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
      size = 1454, 962
[plugins]
[profiles]
  [[default]]
    background_color = "#131926"
    background_darkness = 0.95
    background_type = transparent
    cursor_color = "#aaaaaa"
    cursor_shape = ibeam
    foreground_color = "#d3dae3"
    icon_bell = False
    palette = "#000000:#cd0000:#00cd00:#cdcd00:#0000ee:#cd00cd:#00cdcd:#e5e5e5:#7f7f7f:#ff0000:#00ff00:#ffff00:#5c5cff:#ff00ff:#00ffff:#ffffff"
    scrollback_infinite = True
    scrollback_lines = 1000
    show_titlebar = False
